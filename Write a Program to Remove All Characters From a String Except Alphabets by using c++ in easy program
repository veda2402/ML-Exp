#include <iostream>
#include <string>
using namespace std;

int main() {
    string str;
    cout << "Enter a string: ";
    getline(cin, str);
    
    string result = "";
    
    // Loop through each character in the string
    for (char c : str) {
        // Check if character is an alphabet (a-z or A-Z)
        if ((c >= 'a' && c <= 'z') || (c >= 'A' && c <= 'Z')) {
            result += c;
        }
    }
    
    cout << "String with only alphabets: " << result << endl;
    
    return 0;
}
